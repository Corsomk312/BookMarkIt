<div class="container " id="padding-top">
	<div class="row">
		<div class="page-header">
			<h2>Create a folder</h2>
			<div class="list-author">
    	<h5><%= link_to "Back to Profile", current_user %></h5>
  	</div>
			</div>
		<div class="col-md-4">

			<div class="form-group">
				<%= form_for @list, html: {class: "form-inline", id: "create-list"} do |f| %>
				  <%= f.text_field :name, placeholder: "Folder name", class: 'form-control' %>
				  <%= f.submit "Create", class: "btn btn-default" %>
				<% end %>
			</div>
		</div>
	</div>
</div>
