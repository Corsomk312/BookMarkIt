<div class="container">
	<div class="page-header"><h1>Upload Chrome Bookmark HTML file</h1></div>

			<!-- 	<form class="form-inline"> -->
					<%= form_for @upload, url: upload_index_path, multipart: true do |f| %>

						<div class="form-group">
							<%= f.label :file %>
							<%= f.file_field :upload_file_name, class: 'form-control'%>
						</div>

					  <%= f.submit 'Upload', class: 'btn btn-default' %>
					<% end %>
				<!-- </form> -->
	</div>
</div>
